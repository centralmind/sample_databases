{
    "name": "Gateway Example Codespace",
    "forwardPorts": [9090],
    "portsAttributes": {
      "9090": {
        "label": "App Port",
        "onAutoForward": "openPreview"
      }
    },
    "customizations": {
        "vscode": {
          "settings": {
            "terminal.integrated.defaultProfile.linux": "bash"
          },
          "extensions": []
        }
      },
    "postCreateCommand": "bash .devcontainer/setup.sh",
    "postStartCommand": "echo 'bash .devcontainer/run.sh' >> /home/vscode/.bashrc",
    "containerEnv": {      
      "VAR1": "${VAR1}"
    }
  }
  